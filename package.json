{
  "name": "react-golang-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Golang and Webpack.",
  "repository": "github.com/CDNHammer/react-golang-full-stack",
  "main": "src/backend/backend.go",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && cd src/backend && go build && ./backend",
    "webapp": "webpack serve --mode development --devtool inline-source-map --hot",
    "backend": "nodemon --ext go --exec \"cd src/backend && go build -o backend.exe && backend.exe || exit 1\"",
    "dev": "concurrently \"npm run backend\" \"npm run webapp\""
  },
  "author": "Neal Andrews",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "ace-builds": "^1.5.0",
    "babel-polyfill": "^6.26.0",
    "blueimp-md5": "^2.19.0",
    "fengari-web": "^0.1.4",
    "font-awesome": "^4.7.0",
    "pdfform.js": "^1.0.14",
    "process": "^0.11.10",
    "purecss-sass": "^2.1.0",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-quill": "^2.0.0-beta.4",
    "react-typeahead": "^2.0.0-alpha.8",
    "util": "^0.12.4",
    "webpack": "^5.27.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "node-sass": "^7.0.1",
